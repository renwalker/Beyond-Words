/* ==========================================================================

	!   LESS MIXINS

	Vendor prefixes: to hell with you!

	How to use:

	1. Default parameters:

			.selector {
				.box-shadow();
				}

	2. Custom parameters:

			.selector {
				.border-radius2(10px, 0, 10px, 0);
				}

	3. Complex mixins:

			.selector {
				@border: 1px solid @light_gray;
				.bordered(6px dotted red, none, none, @border);
				}

	========================================================================== */

.debug(@args:1px solid red){
	border: @args;
}

.clearfix(){
	*zoom: 1;
	&:before, &:after { content: ""; display: table; }
	 &:after { clear: both; }
}

.border-radius(@radiusvalue) {
	-webkit-border-radius: @radiusvalue;
		-moz-border-radius: @radiusvalue;
			border-radius: @radiusvalue;
			-ms-border-radius: @radiusvalue;
				-o-border-radius: @radiusvalue;
	}

.border-radius2(@topright: 0, @bottomright: 0, @bottomleft: 0, @topleft: 0) {
	  -webkit-border-top-right-radius: @topright;
	  -webkit-border-bottom-right-radius: @bottomright;
	  -webkit-border-bottom-left-radius: @bottomleft;
	  -webkit-border-top-left-radius: @topleft;
		  -moz-border-radius-topright: @topright;
		  -moz-border-radius-bottomright: @bottomright;
		  -moz-border-radius-bottomleft: @bottomleft;
		  -moz-border-radius-topleft: @topleft;
				border-top-right-radius: @topright;
				border-bottom-right-radius: @bottomright;
				border-bottom-left-radius: @bottomleft;
				border-top-left-radius: @topleft;
	  -moz-background-clip: padding;
	  	-webkit-background-clip: padding-box;
	  		background-clip: padding-box;
}

.radius-circle {
	-webkit-border-radius: 50%;
		-moz-border-radius: 50%;
			border-radius: 50%;
}

.box-shadow(@shadow: 0px 0px 5px ) {
    -webkit-box-shadow: @shadow;
	    -moz-box-shadow: @shadow;
		    box-shadow: @shadow;
	}

.inner-shadow(@shadow: 0px 0px 5px) {
    .box-shadow(inset @shadow);
	}

.rgb_a(@color, @alpha) {
    @alpha_color: hsla(hue(@color), saturation(@color), lightness(@color), @alpha); /* you can use HEX as your color ! */
    @ie_alpha_color: argb(@alpha_color);
    background-color: @color; // Fallback for older browsers
    background-color: @alpha_color;
    // IE hacks
    zoom: 1; // hasLayout
    background-color: transparent\9;
/*
    -ms-filter:  "progid:DXImageTransform.Microsoft.gradient(startColorstr=@{ie_alpha_color}, endColorstr=@{ie_alpha_color})";
        filter: ~"progid:DXImageTransform.Microsoft.gradient(startColorstr=@{ie_alpha_color}, endColorstr=@{ie_alpha_color})";
*/
	}

.placeholder(@color) {
	&::-ms-input-placeholder { color:@color !important; font-style: italic !important; }
	&::-moz-placeholder { color:@color !important;  font-style: italic !important;}
	&::-webkit-input-placeholder { color:@color !important;  font-style: italic !important; }
}

.transition(@args : all .25s ease-in-out) {
	-webkit-transition: @args;
	   -moz-transition: @args;
	     -o-transition: @args;
		  	transition: @args;
}

.transform(@args) {
	-webkit-transform: @args;
		-moz-transform: @args;
			-ms-transform: @args;
				-o-transform: @args;
					transform: @args;
	}


.gradient(
	@begin: black,
	@end: white,
	@switch : 100%
	) {
	background:  @begin;
    background: -webkit-gradient(linear, 0 0, 0 100%, from(@begin), color-stop(@switch, @end));
	background: -moz-linear-gradient(top, @begin, @end @switch);
	background: -o-linear-gradient(top, @begin, @end @switch);
	background: linear-gradient(top, @begin, @end @switch);
	}

.select2-gradient(){
	background-image: -webkit-gradient(linear, left bottom, left top, color-stop(0, @light_gray), color-stop(0.9, white));
    background-image: -webkit-linear-gradient(center bottom, @light_gray 0%, white 90%);
    background-image: -moz-linear-gradient(center bottom, @light_gray 0%, white 90%);
    background-image: -o-linear-gradient(bottom, @light_gray 0%, white 90%);
    background-image: -ms-linear-gradient(top, @light_gray 0%,#ffffff 90%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#E9E6E0', endColorstr='#ffffff',GradientType=0 );
    background-image: linear-gradient(top, @light_gray 0%,#ffffff 90%);
}


.bordered(
	@top: none,
	@right: none,
	@bottom: none,
	@left: none,
	) {
		  border-top: @top;
		  border-right: @right;
		  border-bottom: @bottom;
		  border-left: @left;
	}

.opacit_y(@opacity: 0.5) {
  -webkit-opacity: @opacity;
	   -moz-opacity: @opacity;
		  -khtml-opacity: @opacity;
			  opacity: @opacity;
			  @ie: @opacity*100;
				    //-ms-filter: ~'"progid:DXImageTransform.Microsoft.Alpha(Opacity=@{ie})"';
				    //filter: ~"alpha(opacity=@{ie})";
			  		filter: alpha(opacity=@ie);
}


// weight rendering of custom fonts when on lower contrasting background.  ?ie
.font-smoothing( @antialias: subpixel-antialiased ) {
	-webkit-font-smoothing: @antialias;
		-moz-font-smooth: @antialias;
			 font-smooth: @antialias;
}


